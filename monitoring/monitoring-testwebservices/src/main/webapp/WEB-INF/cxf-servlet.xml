<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xmlns:soap="http://cxf.apache.org/bindings/soap"
	xsi:schemaLocation="
         http://www.springframework.org/schema/beans 
         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
         http://cxf.apache.org/jaxws
         http://cxf.apache.org/schemas/jaxws.xsd">

	<jaxws:endpoint id="slow"
		implementor="at.sti2.ngsee.monitoring.testws.TestWebServiceSlow" address="/slow">
	</jaxws:endpoint>

	<jaxws:endpoint id="big"
		implementor="at.sti2.ngsee.monitoring.testws.TestWebServiceBigPayload" address="/big">
	</jaxws:endpoint>

	<jaxws:endpoint id="random"
		implementor="at.sti2.ngsee.monitoring.testws.TestWebServiceRandom" address="/random">
	</jaxws:endpoint>

	<jaxws:endpoint id="constant"
		implementor="at.sti2.ngsee.monitoring.testws.TestWebServiceConstant" address="/constant">
	</jaxws:endpoint>
</beans>
