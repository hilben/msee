	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.axis2</groupId>
				<artifactId>axis2-java2wsdl-maven-plugin</artifactId>
				<version>1.6.2</version>
				<executions>
					<execution>
						<id>install</id>
						<phase>install</phase>
						<goals>
							<goal>java2wsdl</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<className>at.sti2.msee.registration.api.ServiceRegistration</className>
					<outputFileName>${project.build.directory}/../src/main/resources/RegistrationAPI.wsdl</outputFileName>
					<wsdlVersion>2</wsdlVersion>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.axis2</groupId>
				<artifactId>axis2-wsdl2code-maven-plugin</artifactId>
				<version>1.6.2</version>
				<executions>
					<execution>
						<id>install</id>
						<phase>install</phase>
						<goals>
							<goal>wsdl2code</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<classpathElements>${project.build.outputDirectory}</classpathElements>
					<wsdlFile>${project.build.directory}/../src/main/resources/RegistrationAPI.wsdl</wsdlFile>
					<wsdlVersion>2</wsdlVersion>
					<databindingName>adb</databindingName>
					<packageName>at.sti2.msee.registration</packageName>
					<outputDirectory>${project.build.directory}/../src/test/</outputDirectory>
					<targetSourceFolderLocation>java</targetSourceFolderLocation>
					<databindingName>xmlbeans</databindingName>
				</configuration>
			</plugin>

