<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
        <!ENTITY msm "http://cms-wg.sti2.org/ns/minimal-service-model#">
        <!ENTITY wl "http://www.wsmo.org/ns/wsmo-lite#">
        <!ENTITY sawsdl "http://www.w3.org/ns/sawsdl#">
        <!ENTITY rest "http://www.wsmo.org/ns/hrests#">
        <!ENTITY wsdl "http://www.w3.org/ns/wsdl-rdf#" >
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:http="http://www.w3.org/2008/http-methods#"
        xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
        xmlns:sawsdl="&sawsdl;"
        xmlns:rest="&rest;"
        xmlns:wl="&wl;"
        xmlns:wsdl="&wsdl;"
        xmlns:msm="&msm;">

        <rdfs:Class rdf:about="&wl;Ontology">
                <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Ontology" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;InterfaceOperation">
                <rdfs:subClassOf rdf:resource="&msm;Operation" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;InputMessage">
                <rdfs:subClassOf rdf:resource="&msm;MessageContent" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;OutputMessage">
                <rdfs:subClassOf rdf:resource="&msm;MessageContent" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;QName">
                <rdfs:subClassOf rdf:resource="&msm;MessagePart" />
        </rdfs:Class>

        <!-- Begin TODO: Check this! Wrong subPropertyOf relation... -->
        <rdfs:Property rdf:about="&wsdl;interfaceMessageReference">
                <rdfs:subPropertyOf rdf:resource="&msm;hasInput" />
        </rdfs:Property>

        <rdfs:Property rdf:about="&wsdl;interfaceMessageReference">
                <rdfs:subPropertyOf rdf:resource="&msm;hasOutput" />
        </rdfs:Property>
        <!-- End TODO -->

        <rdfs:Property rdf:about="&wsdl;interfaceFaultReference">
                <rdfs:subPropertyOf rdf:resource="&msm;hasFault" />
        </rdfs:Property>

        <rdfs:Property rdf:about="#wsdlDescription">
                <rdfs:label>wsdl Description</rdfs:label>
                <vs:term_status>testing</vs:term_status>
                <rdfs:domain rdf:resource="&msm;Service" />
        </rdfs:Property>

 </rdf:RDF>
