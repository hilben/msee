<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
        <!ENTITY wl "http://www.wsmo.org/ns/wsmo-lite#">
        <!ENTITY sawsdl "http://www.w3.org/ns/sawsdl#">
        <!ENTITY rest "http://www.wsmo.org/ns/hrests#">
        <!ENTITY wsdl "http://www.w3.org/ns/wsdl-rdf#" >
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:sawsdl="&sawsdl;"
	xmlns:http="http://www.w3.org/2008/http-methods#"
        xmlns:vs="http://www.w4.org/2003/06/sw-vocab-status/ns#"
	xmlns:wl="&wl;"
	xmlns:rest="&rest;">

	<rdfs:Class rdf:about="http://www.wsmo.org/ns/wsmo-lite#Ontology">
		<rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Ontology" />
	</rdfs:Class>

	<rdf:Description rdf:about="http://www.wsmo.org/ns/wsmo-lite#FunctionalClassificationRoot">
		<rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class" />
	</rdf:Description>

	<rdfs:Class rdf:about="http://www.wsmo.org/ns/wsmo-lite#NonFunctionalParameter" />
	<rdfs:Class rdf:about="http://www.wsmo.org/ns/wsmo-lite#Condition" />
	<rdfs:Class rdf:about="http://www.wsmo.org/ns/wsmo-lite#Effect" />

	<rdfs:Class rdf:about="http://www.w3.org/2008/http-methods#Method">
		<rdfs:subClassOf rdf:resource="http://www.wsmo.org/ns/hrests#Method" />
	</rdfs:Class>

	<rdfs:Class rdf:about="#Service" />

	<rdfs:Class rdf:about="#Operation" />

	<rdfs:Class rdf:about="#MessagePart" />

	<rdfs:Class rdf:about="#MessageContent">
		<rdfs:subClassOf rdf:resource="#MessagePart" />
	</rdfs:Class>

	<rdfs:Class rdf:about="http://www.wsmo.org/ns/hrests#URITemplate" />

	<rdf:Property rdf:about="http://www.wsmo.org/ns/wsmo-lite#usesOntology">
		<rdfs:domain rdf:resource="#Service" />
		<rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso" />
	</rdf:Property>

	<rdf:Property rdf:about="#hasOperation">
		<rdfs:domain rdf:resource="#Service" />
		<rdfs:range rdf:resource="#Operation" />
	</rdf:Property>

        <!--
           - Substitution for hasInput, hasOutput, hasInputFault and hasOutputFault
          -->
	<rdf:Property rdf:about="#hasMessageContent">
		<rdfs:domain rdf:resource="#Operation" />
		<rdfs:range rdf:resource="#MessageContent" />
	</rdf:Property>

	<rdf:Property rdf:about="http://www.wsmo.org/ns/hrests#hasAddress">
		<rdfs:domain rdf:resource="#Operation" />
		<rdfs:range rdf:resource="http://www.wsmo.org/ns/hrests#URITemplate" />
	</rdf:Property>

	<rdf:Property rdf:about="http://www.wsmo.org/ns/hrests#hasMethod">
		<rdfs:domain rdf:resource="#Operation" />
		<rdfs:range rdf:resource="http://www.wsmo.org/ns/hrests#Method" />
	</rdf:Property>

	<rdf:Property rdf:about="http://www.w3.org/ns/sawsdl#modelReference" />

	<rdf:Property rdf:about="http://www.w3.org/ns/sawsdl#liftingSchemaMapping" />

	<rdf:Property rdf:about="http://www.w3.org/ns/sawsdl#loweringSchemaMapping" />

	<rdf:Property rdf:about="#hasPart">
		<rdfs:domain rdf:resource="#MessagePart" />
		<rdfs:range rdf:resource="#MessagePart" />
	</rdf:Property>

	<rdf:Property rdf:about="#hasName">
		<rdfs:domain rdf:resource="#MessagePart" />
		<rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal" />
	</rdf:Property>

	<rdf:Property rdf:about="#hasOptionalPart">
		<rdfs:subPropertyOf rdf:resource="#hasPart" />
	</rdf:Property>

	<rdf:Property rdf:about="#hasMandatoryPart">
		<rdfs:subPropertyOf rdf:resource="#hasPart" />
	</rdf:Property>

        <!--
           - Start Extensions
          -->
        <rdfs:Class rdf:about="#Input">
                <rdfs:subClassOf rdf:resource="#MessageContent"/>
        </rdfs:Class>

        <rdfs:Class rdf:about="#Output">
                <rdfs:subClassOf rdf:resource="#MessageContent"/>
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;InterfaceOperation">
                <rdfs:subClassOf rdf:resource="#Operation" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;InputMessage">
                <rdfs:subClassOf rdf:resource="#Input" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;OutputMessage">
                <rdfs:subClassOf rdf:resource="#Output" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&wsdl;QName">
                <rdfs:subClassOf rdf:resource="#MessagePart" />
        </rdfs:Class>

        <rdfs:Property rdf:about="&wsdl;interfaceMessageReference">
                <rdfs:subPropertyOf rdf:resource="#hasInput" />
        </rdfs:Property>

        <rdfs:Property rdf:about="&wsdl;interfaceMessageReference">
                <rdfs:subPropertyOf rdf:resource="#hasMessageContent" />
        </rdfs:Property>

        <rdfs:Property rdf:about="&wsdl;interfaceFaultReference">
                <rdfs:subPropertyOf rdf:resource="#hasMessageContent" />
        </rdfs:Property>

        <rdfs:Property rdf:about="#wsdlDescription">
                <rdfs:label>wsdl Description</rdfs:label>
                <vs:term_status>testing</vs:term_status>
                <rdfs:domain rdf:resource="#Service" />
                <rdfs:range rdf:resource="#Description" />
        </rdfs:Property>

</rdf:RDF>
