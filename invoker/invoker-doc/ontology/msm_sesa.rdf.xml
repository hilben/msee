<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
        <!ENTITY msm "http://cms-wg.sti2.org/ns/minimal-service-model#">
        <!ENTITY wl "http://www.wsmo.org/ns/wsmo-lite#">
        <!ENTITY sawsdl "http://www.w3.org/ns/sawsdl#">
        <!ENTITY rest "http://www.wsmo.org/ns/hrests#">
]>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
        xmlns:owl="http://www.w3.org/2002/07/owl#"
        xmlns:http="http://www.w3.org/2008/http-methods#"
        xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
        xmlns:sawsdl="&sawsdl;"
        xmlns:rest="&rest;"
        xmlns:wl="&wl;"
        xmlns:msm="&msm;">

        <rdfs:Class rdf:about="&wl;Ontology">
                <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Ontology" />
        </rdfs:Class>

        <rdf:Description rdf:about="&wl;FunctionalClassificationRoot">
                <rdfs:subClassOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class" />
        </rdf:Description>

        <rdfs:Class rdf:about="&wl;NonFunctionalParameter" />
        <rdfs:Class rdf:about="&wl;Condition" />
        <rdfs:Class rdf:about="&wl;Effect" />

        <rdfs:Class rdf:about="http://www.w3.org/2008/http-methods#Method">
                <rdfs:subClassOf rdf:resource="&rest;Method" />
        </rdfs:Class>

        <rdfs:Class rdf:about="#Service" />

        <rdfs:Class rdf:about="#Operation" />

        <rdfs:Class rdf:about="#MessagePart" />

        <rdfs:Class rdf:about="#MessageContent">
                <rdfs:subClassOf rdf:resource="#MessagePart" />
        </rdfs:Class>

        <rdfs:Class rdf:about="&rest;URITemplate" />

        <rdf:Property rdf:about="&wl;usesOntology">
                <rdfs:domain rdf:resource="#Service" />
                <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#seeAlso" />
        </rdf:Property>

        <rdf:Property rdf:about="#hasOperation">
                <rdfs:domain rdf:resource="#Service" />
                <rdfs:range rdf:resource="#Operation" />
                <!--
                   - Extension for STI SESA Framework. 09.05.2011
                  -->
                 <rdfs:domain rdf:resource="#Binding" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasInput">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="#MessageContent" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasOutput">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="#MessageContent" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasInputFault">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="#MessageContent" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasOutputFault">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="#MessageContent" />
         </rdf:Property>

         <rdf:Property rdf:about="&rest;hasAddress">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="&rest;URITemplate" />
         </rdf:Property>

         <rdf:Property rdf:about="&rest;hasMethod">
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="&rest;Method" />
         </rdf:Property>

         <rdf:Property rdf:about="&sawsdl;modelReference" />

         <rdf:Property rdf:about="&sawsdl;liftingSchemaMapping" />

         <rdf:Property rdf:about="&sawsdl;loweringSchemaMapping" />

         <rdf:Property rdf:about="#hasPart">
                 <rdfs:domain rdf:resource="#MessagePart" />
                 <rdfs:range rdf:resource="#MessagePart" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasName">
                 <rdfs:domain rdf:resource="#MessagePart" />
                 <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Literal" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasOptionalPart">
                 <rdfs:subPropertyOf rdf:resource="#hasPart" />
         </rdf:Property>

         <rdf:Property rdf:about="#hasMandatoryPart">
                 <rdfs:subPropertyOf rdf:resource="#hasPart" />
         </rdf:Property>

         <!--
           - Extension for STI SESA Framework. 09.05.2011
          -->

         <rdfs:Class rdf:about="#SoapAction">
                 <rdfs:label>SOAP Action</rdfs:label>
                 <rdfs:comment>Describes the SOAPAction field used in the HTTP Header</rdfs:comment>
                 <vs:term_status>unstable</vs:term_status>
         </rdfs:Class>

         <rdf:Property rdf:about="#hasSoapAction">
                 <rdfs:label>has SOAP Action</rdfs:label>
                 <rdfs:domain rdf:resource="#Operation" />
                 <rdfs:range rdf:resource="#SoapAction" />
                 <vs:term_status>unstable</vs:term_status>
         </rdf:Property>

         <rdfs:Class rdf:about="#Binding">
                 <rdfs:label>Binding</rdfs:label>
                 <rdfs:comment>Describes a SOAP Binding URL</rdfs:comment>
                 <vs:term_status>unstable</vs:term_status>
         </rdfs:Class>

         <rdf:Property rdf:about="#hasBinding">
                 <rdfs:label>has Binding</rdfs:label>
                 <rdfs:domain rdf:resource="#Service" />
                 <rdfs:range rdf:resource="#Binding" />
                 <vs:term_status>unstable</vs:term_status>
         </rdf:Property>

         <rdf:Property rdf:about="#hasOperationQName">
                 <rdfs:label>has Binding</rdfs:label>
                 <rdfs:domain rdf:resource="#Service" />
                 <rdfs:range rdf:resource="#Binding" />
                 <vs:term_status>unstable</vs:term_status>
         </rdf:Property>

 </rdf:RDF>
